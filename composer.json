{
    "name": "sfneal/caching",
    "description": "Traits & interfaces for utilizing cache mechanisms to store frequently retrieved data.",
    "keywords": [
        "sfneal",
        "caching"
    ],
    "homepage": "https://github.com/sfneal/caching",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Stephen Neal",
            "email": "stephen.neal14@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "sfneal/redis-helpers": "^1.4|^2.0"
    },
    "require-dev": {
        "josiasmontag/laravel-redis-mock": ">=1.2.6",
        "orchestra/testbench": ">=3.8.0",
        "phpunit/phpunit": ">=6.5.14",
        "scrutinizer/ocular": "^1.8",
        "sfneal/currency": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Sfneal\\Caching\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sfneal\\Caching\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-composer": "composer test",
        "test-both": "composer test && composer docker-test",
        "test-all": "composer update-test && composer docker-test",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "docker-test": "bash scripts/boot.sh",
        "docker-test-lowest": "bash scripts/boot.sh --prefer-lowest",
        "docker-down": "bash scripts/down.sh",
        "upload-coverage": "php vendor/bin/ocular code-coverage:upload --format=php-clover phpunit.xml.dist",
        "update-test": "composer --prefer-stable update && composer test && composer update --prefer-lowest && composer test && composer update"
    },
    "config": {
        "sort-packages": true
    }
}
